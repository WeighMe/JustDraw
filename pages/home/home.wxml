<!--pages/home/home.wxml-->
<!-- <view class="container" > -->
<view class="whole" style='background:url("https://upload-images.jianshu.io/upload_images/9653454-e1eaaed42013d3e4.jpg?imageMogr2/auto-orient/");-webkit-background-size:cover;' bindtouchstart='touchstart' bindtouchend='touchend' focus="{{focus}}">
  <!--排行榜-->
  <view class=".page_tab" style='background:url("https://upload-images.jianshu.io/upload_images/9653454-7f5a3b9cc76742a9.jpg?imageMogr2/auto-orient/");-webkit-background-size:cover;{{translate}}' wx:if='{{display}}' animation='{{animationP}}'>  
    <view class='v_title'>
      <text class='t_title'>排行榜</text>
      <view class='v_cancel'>
        <icon class='icon_cancel' type='cancel' bindtap='closePage' color='rgb(240,220,200)'></icon>
      </view>  
    </view>
    <scroll-view class='.page_content' scroll-y='true'>
      <view class='v_item'>
        <text class='t_number'>1</text>
        <view class='v_info'>
          <image class='i_userimage'></image>
          <text class='t_username'>明贤大佬</text>
        </view>
        <text class='t_score'>666</text>
      </view>
    <view class='v_item'>
      <text class='t_number'>2</text>
      <view class='v_info'>
        <image class='i_userimage'></image>
        <text class='t_username'>明贤大佬</text>
      </view>
      <text class='t_score'>776</text>
    </view>
    <view class='v_item'>
      <text class='t_number'>3</text>
      <view class='v_info'>
        <image class='i_userimage'></image>
        <text class='t_username'>明贤大佬</text>
      </view>
      <text class='t_score'>666</text>
    </view>
    <view class='v_item'>
      <text class='t_number'>4</text>
      <view class='v_info'>
        <image class='i_userimage'></image>
        <text class='t_username'>明贤大佬</text>
      </view>
      <text class='t_score'>666</text>
    </view>
      <view class='v_item'>
        <text class='t_number'>5</text>
        <view class='v_info'>
          <image class='i_userimage'></image>
          <text class='t_username'>明贤大佬</text>
        </view>
        <text class='t_score'>666</text>
      </view> 
    </scroll-view>
  </view>
  <!--主界面-->
  <view class="v_btnCtnr">
    <view class='btn_flex'>
      <view class="btntoroom"><image animation="{{btnAnimationCre}}" class="btn" bindtap='jump2Room' src='https://upload-images.jianshu.io/upload_images/9653454-c82e75016e7a27dd.png?imageMogr2/auto-orient/'></image></view>
      <view class="btntoroom"><image animaiton="{{btnAnimationRan}}" class="btn" src='https://upload-images.jianshu.io/upload_images/9653454-34bd41b8171501df.png?imageMogr2/auto-orient/' bindtap='jump2Ran'></image></view>
      <view class='btntoroom'><image animaiton="{{btnAnimatioJoi}}" class='btn' src='https://upload-images.jianshu.io/upload_images/9653454-4c89f6165a748430.png?imageMogr2/auto-orient/' bindtap='showWin'></image></view>
    </view>
  </view>
  <view class='v_roll' catchtap='showWin'></view>
  <view class='vw_winCtnr' catchtap='hideWin' wx:if='{{setShow}}'>
    <view animation="{{animationData}}" class="v_enterRoom" catchtap='stop' wx:if='{{setShow}}' style='background:url("https://upload-images.jianshu.io/upload_images/9653454-9a14d370b25647ca.jpg?imageMogr2/auto-orient/");-webkit-background-size:cover;'>
      <form bindsubmit='enterRoom'>
        <input class='i_room' name="roomNumber" focus='true' confirm-type='go' maxlength='8' bindconfirm='enterRoom' placeholder-style="font-size:15px" placeholder="     请输入房间号" placeholder-class="phcolor" auto-focus></input>
        <view class='btn_ctn'>
          <button class='btn_yes' form-type="submit" plain='true' style='color:rgb(240,220,200);border:2px solid rgb(240,220,200);border-radius:5px;opacity:0.8'>确 认</button>
          <button class='btn_no' bindtap='hideWin' plain='false' style='color:rgb(240,220,200);border:2px solid rgb(240,220,200);border-radius:5px;opacity:0.8'>取 消</button>
        </view>
      </form>
    </view>
  </view>
</view>
